pipeline{
    agent any
   
    parameters{
    choice choices: ['development', 'master', 'qa', 'uat'], name: 'branch'
    }
   
    tools{
        maven "maven3.8.4"
    }
    
    stages{
        stage('Checkoutcode'){
        steps{
        git branch: "${branch}", credentialsId: 'github_new_creds', url: 'https://github.com/devopstools12/maven-web-application.git'                
        }
        }
    }
}
